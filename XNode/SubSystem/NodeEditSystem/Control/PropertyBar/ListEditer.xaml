<local:PropertyBarBase x:Class="XNode.SubSystem.NodeEditSystem.Control.PropertyBar.ListEditer"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:XNode.SubSystem.NodeEditSystem.Control.PropertyBar"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid>
        <StackPanel Orientation="Vertical" TextBlock.Foreground="White">
            <StackPanel x:Name="Stack_CaseList" Orientation="Vertical">
            </StackPanel>
            <Grid x:Name="Grid_Tool" Background="#3C3C3C" HorizontalAlignment="Left" Width="25" Height="25" Margin="0,10,0,0">
                <Button x:Name="Tool_Add" Style="{StaticResource ToolBarButton}" Click="Tool_Add_Click">
                    <Button.Content>
                        <Image Source="../../../../Assets/Icon15/Add.png" Width="15" Height="15"/>
                    </Button.Content>
                </Button>
            </Grid>
        </StackPanel>
    </Grid>
</local:PropertyBarBase>